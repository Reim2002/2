function MakeForm(PART,TYPE)
	if TYPE == "Cyl" then
		local MSH = IT("CylinderMesh",PART)
	elseif TYPE == "Ball" then
		local MSH = IT("SpecialMesh",PART)
		MSH.MeshType = "Sphere"
	elseif TYPE == "Wedge" then
		local MSH = IT("SpecialMesh",PART)
		MSH.MeshType = "Wedge"
	end
end

Debris = game:GetService("Debris")

function CastProperRay(StartPos, EndPos, Distance, Ignore)
	local DIRECTION = CF(StartPos,EndPos).lookVector
	return Raycast(StartPos, DIRECTION, Distance, Ignore)
end

function FixLighting()
	game.Lighting.Ambient = Color3.fromRGB(138,138,138)
	game.Lighting.OutdoorAmbient = Color3.fromRGB(128,128,128)
	game.Lighting.FogColor = Color3.fromRGB(192,192,192)
	game.Lighting.TimeOfDay = "7:00:00"
	game.Lighting.ExposureCompensation = 0
	game.Lighting.FogEnd = 100000
	game.Lighting.Brightness = 2
	workspace.CurrentCamera.FieldOfView = 70 - sick.PlaybackLoudness/90
end
local VISUALS = game.Lighting
local VISUALSSz = game.Lighting
local Bloom = script.Bloom
Bloom.Parent = VISUALS
script["C-Correction"].Parent = VISUALS
wait(0.1)
coroutine.resume(coroutine.create(function()
	while true do
		wait()
		if Idk == true then
			VISUALS.FogColor = Color3.fromRGB(255, 255, 255)
			VISUALS.Ambient = Color3.fromRGB(255, 255, 255)
			VISUALS.OutdoorAmbient = Color3.fromRGB(255, 255, 255)
			VISUALS.TimeOfDay = 0
			Bloom.Intensity = 0 + sick.PlaybackLoudness/500
			VISUALSSz.ExposureCompensation = 0 + sick.PlaybackLoudness/130
			VISUALSSz.FogEnd = sick.PlaybackLoudness
			VISUALSSz.Brightness = 2
			VISUALS.EnvironmentDiffuseScale = 0 + sick.PlaybackLoudness/100
			VISUALS["C-Correction"].Contrast= 0.1 + sick.PlaybackLoudness/1000
			workspace.CurrentCamera.FieldOfView = 70 - sick.PlaybackLoudness/100
		else
			FixLighting()
		end
	end
end))

function ToggleSome()
	if Idk == true then

		Idk = false
		AdvancedChatfunc("Lightning = Off")
	else
		Idk = true
		AdvancedChatfunc("Lightning = On")
	end
end



function unanchor()
	if UNANCHOR == true then
		g = Character:GetChildren()
		for i = 1, #g do
			if g[i].ClassName == "Part" then
				g[i].Anchored = false
			end
		end
	end
end

