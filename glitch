-- NORMAL POS
nx = 0 -- x
ny = 0 -- y
nz = 1.5 -- z
-- NORMAL POS
-- NORMAL ROTATION
rx = 0 -- x
ry = 0 -- y
rz = 0 -- z
-- NORMAL ROTATION
-- mesh destroying
-- pos animate
-- KEEP ALL ZERO TO NOT ANIMATE
x = 0 -- pos x
y = 0 -- pos y
z = 0 -- pos z
-- KEEP ALL ZERO TO NOT ANIMATE
-- pos animate
-- ROTATION ANIMATE
xr = 0 -- x
xy = 0 -- y
xz = 20 -- z
-- ROTATION ANIMATE
-- ROTATION SPEED
speedxx = 0 -- x
speedxy = 0 -- y
speedxz = 0.4 -- z
-- ROTATION SPEED
-- speed
-- KEEP ALL ZERO TO NOT ANIMATE
speedx = 5 -- x
speedy = 5-- y
speedz = 5 -- z
-- KEEP ALL ZERO TO NOT ANIMATE
-- speed
