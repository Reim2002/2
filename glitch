			game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,-5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
			NK.C0 = NK.C0:lerp(NKC0,Alpha)
			RJ.C0 = RJ.C0:lerp(RJC0*CFrame.new(0-0.05*COS(SINE/22),4+3*COS(SINE/60),0-0.05*COS(SINE/22))*CFrame.Angles(math.rad(-40),math.rad(0-10*SIN(SINE/22)),math.rad(0)),Alpha)
			RS.C0 = RS.C0:lerp(RSC0*CFrame.new(-0.1,-0.25,0.2)*CFrame.Angles(math.rad(-45),math.rad(-25-4*SIN(SINE/22)),math.rad(40)),Alpha)
			LS.C0 = LS.C0:lerp(LSC0*CFrame.new(0.1,-0.25,0.2)*CFrame.Angles(math.rad(-45),math.rad(25+4*SIN(SINE/22)),math.rad(-40)),Alpha)
			RH.C0 = RH.C0:lerp(RHC0*CFrame.new(0+0.05*COS(SINE/22),0.5,-0.8+0.05*COS(SINE/22))*CFrame.Angles(math.rad(-5),math.rad(-15-4*SIN(SINE/22)),math.rad(15)),Alpha)
			LH.C0 = LH.C0:lerp(LHC0*CFrame.new(-0.2+0.05*COS(SINE/22),-0.2,-0.2+0.05*COS(SINE/22))*CFrame.Angles(math.rad(0),math.rad(15+4*SIN(SINE/22)),math.rad(-0)),Alpha)
            elseif mode == "NIGGA" then
            			
			spin2()
			game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,-5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
			NK.C0 = NK.C0:lerp(NKC0,Alpha)
			RJ.C0 = RJ.C0:lerp(RJC0*CFrame.new(0-0.05*COS(SINE/22),2+1*COS(SINE/60),0-0.05*COS(SINE/22))*CFrame.Angles(math.rad(-40),math.rad(0-10*SIN(SINE/22)),math.rad(0)),Alpha)
			RS.C0 = RS.C0:lerp(RSC0*CFrame.new(-0.1,-0.25,0.2)*CFrame.Angles(math.rad(-45),math.rad(-25-4*SIN(SINE/22)),math.rad(40)),Alpha)
			LS.C0 = LS.C0:lerp(LSC0*CFrame.new(0.1,-0.25,0.2)*CFrame.Angles(math.rad(-45),math.rad(25+4*SIN(SINE/22)),math.rad(-40)),Alpha)
			RH.C0 = RH.C0:lerp(RHC0*CFrame.new(0+0.05*COS(SINE/22),0.5,-0.8+0.05*COS(SINE/22))*CFrame.Angles(math.rad(-5),math.rad(-15-4*SIN(SINE/22)),math.rad(15)),Alpha)
			LH.C0 = LH.C0:lerp(LHC0*CFrame.new(-0.2+0.05*COS(SINE/22),-0.2,-0.2+0.05*COS(SINE/22))*CFrame.Angles(math.rad(0),math.rad(15+4*SIN(SINE/22)),math.rad(-0)),Alpha)
        	elseif mode == "MAGICIAN" then
			NK.C0 = NK.C0:lerp(NKC0,Alpha)
			game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,-5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
			RJ.C0 = RJ.C0:lerp(RJC0*CFrame.new(0-0.05*COS(SINE/22),10+5*COS(SINE/60),0-0.05*COS(SINE/22))*CFrame.Angles(math.rad(-40),math.rad(0-10*SIN(SINE/22)),math.rad(0)),Alpha)
			RS.C0 = RS.C0:lerp(RSC0*CFrame.new(-0.1,-0.25,0.2)*CFrame.Angles(math.rad(-45),math.rad(-25-4*SIN(SINE/22)),math.rad(40)),Alpha)
			LS.C0 = LS.C0:lerp(LSC0*CFrame.new(0.1,-0.25,0.2)*CFrame.Angles(math.rad(-45),math.rad(25+4*SIN(SINE/22)),math.rad(-40)),Alpha)
			RH.C0 = RH.C0:lerp(RHC0*CFrame.new(0+0.05*COS(SINE/22),0.5,-0.8+0.05*COS(SINE/22))*CFrame.Angles(math.rad(-5),math.rad(-15-4*SIN(SINE/22)),math.rad(15)),Alpha)
			LH.C0 = LH.C0:lerp(LHC0*CFrame.new(-0.2+0.05*COS(SINE/22),-0.2,-0.2+0.05*COS(SINE/22))*CFrame.Angles(math.rad(0),math.rad(15+4*SIN(SINE/22)),math.rad(-0)),Alpha)
        	elseif mode == "NOBODY" then
                        spin()
            att2.Parent = c["Torso"]
            att2.Position = Vector3.new(0,0,0.5)
            att2.Orientation = Vector3.new(-0, 90, 0)
			game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,-5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
			LH.C0 = LH.C0:lerp(LHC0*CFrame.new(0,0-movement/15*math.cos(Sine/wsVal)/2,(-.1+movement/15*math.cos(Sine/wsVal))*(.5+.5*forwardvelocity))*CFrame.Angles((math.rad(-10*forwardvelocity+Change*5-movement*math.cos(Sine/wsVal))+-(movement/10)*math.sin(Sine/wsVal))*forwardvelocity,0,(math.rad(Change*5-movement*math.cos(Sine/wsVal))+-(movement/10)*math.sin(Sine/wsVal))*(sidevec/(Hum.WalkSpeed*2))),Alpha)
			RH.C0 = RH.C0:lerp(RHC0*CFrame.new(0,0+movement/15*math.cos(Sine/wsVal)/2,(-.1-movement/15*math.cos(Sine/wsVal))*(.5+.5*forwardvelocity))*CFrame.Angles((math.rad(-10*forwardvelocity+change*5+movement*math.cos(Sine/wsVal))+(movement/10)*math.sin(Sine/wsVal))*forwardvelocity,-0,(math.rad(Change*5+movement*math.cos(Sine/wsVal))+(movement/10)*math.sin(Sine/wsVal))*(sidevec/(Hum.WalkSpeed*2))),Alpha)
			RJ.C0 = RJ.C0:lerp(RJC0*CFrame.new(0,.05+Change/4*math.cos(Sine/(wsVal/2)),0)*CFrame.Angles(math.rad(-(Change*20)-movement/20*math.cos(Sine/(wsVal/2)))*forwardvelocity,math.rad(0+5*math.cos(Sine/wsVal)),math.rad(-(Change*20)-movement/20*math.cos(Sine/(wsVal/2)))*sidevelocity+math.rad(0-1*math.cos(Sine/wsVal))),Alpha)
			NK.C0 = NK.C0:lerp(NKC0,Alpha)
			LS.C0 = LS.C0:lerp(LSC0*CFrame.new(0,0,0)*CFrame.Angles(math.rad(0+55*(movement/8)*math.sin(Sine/wsVal))*forwardvelocity,0,0),Alpha)
			RS.C0 = RS.C0:lerp(RSC0*CFrame.new(0,0,0)*CFrame.Angles(math.rad(0-55*(movement/8)*math.sin(Sine/wsVal))*forwardvelocity,0,0),Alpha)
        end
	end
	unanchor()
	if Rooted == false then
		Disable_Jump = false
		Humanoid.WalkSpeed = Speed
	elseif Rooted == true then
		Disable_Jump = true
		Humanoid.WalkSpeed = 0
	end
	sick.Looped = true
	sick.Pitch = 1
	sick.Volume = .8
	sick:Resume()
	sick.Parent = Torso
	Humanoid.JumpPower = Jump
end
