		    RJ.C0 = Clerp(RJ.C0,RJC0 * CF(0, 0, 0) * ANGLES(math.rad(-(change * 20) - movement / 20 * math.cos(SINE / 2)) * (lookvector / (Humanoid.WalkSpeed * 2)) + math.rad(velocityYFall3 / 10), math.rad(0), math.rad(-(change * 20) - movement / 20 * math.cos(SINE / 2)) * (rightvector / (Humanoid.WalkSpeed * 2))), .1)
			RS.C0 = Clerp(RS.C0, CF(1.5, 0.55 - velocityYFall3 / 70, 0) * ANGLES(math.rad(-velocityYFall3 / 10), math.rad(0), math.rad(-velocityYFall)), .2)
			LS.C0 = Clerp(LS.C0, CF(-1.5, 0.55 - velocityYFall3 / 70, 0) * ANGLES(math.rad(-velocityYFall3 / 10), math.rad(0), math.rad(velocityYFall)), .2)
			RH.C0 = Clerp(RH.C0, CF(0.5, 0, -0.8) * ANGLES(math.rad(-25), math.rad(0), math.rad(2)), .1)
			LH.C0 = Clerp(LH.C0, CF(-0.5, -0.925, 0) * ANGLES(math.rad(-35), math.rad(0), math.rad(-2)), .1)	
		end 
	elseif TORSOVELOCITY < 1 and HITFLOOR ~= nil then
		ANIM = "Idle"
		if ATTACK == false and mode == "NIGGERS" then

            att2.Parent = c["Right Arm"]
att2.Position = Vector3.new(0,-1,-3.7)
att2.Orientation = Vector3.new(130, 0, -0)

            att1.Parent = c["Torso"]
            att1.Position = Vector3.new(0,-25,0)
            att1.Orientation = Vector3.new(-0, 0, 0)

			local Alpha = .1
			NK.C0 = NK.C0:lerp(NKC0,Alpha)
			game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['InternationalFedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,-5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
			game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0 = game.Players.LocalPlayer.Character['International Fedora'].Handle.AccessoryWeld.C0:lerp(RJC0*CFrame.new(0,2,5)*CFrame.Angles(math.rad(0-10*SIN(SINE/22)),math.rad(0-360*SIN(SINE/22)),math.rad(0-10*SIN(SINE/22))),Alpha)
			RJ.C0 = RJ.C0:lerp(RJC0*CFrame.new(0-0.05*COS(SINE/22),-0.2-0.1*COS(SINE/22),0-0.05*COS(SINE/22))*CFrame.Angles(math.rad(-8),math.rad(10-6*SIN(SINE/22)),math.rad(0)),Alpha)
			RS.C0 = RS.C0:lerp(RSC0*CFrame.new(0,0,0)*CFrame.Angles(math.rad(0),math.rad(-20),math.rad(10+6*SIN(SINE/22))),Alpha)
