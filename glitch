function mode1()
	if mode == "NIGGERS" then
		sick.TimePosition = 15
		sick.SoundId = "rbxassetid://9042666762"
sick.Name = ""
		mode = "begone"
		
tecks.Text = "C H I L L"
tecks.Font = "GothamBlack"
		
	elseif mode == "begone" then
		mode = "NIGGERS"
		
tecks.Text = "ARCADE KILLER"
tecks.Font = "Arcade"
		
        sick.SoundId = "rbxassetid://9048378262"
        sick.PlaybackSpeed  = 2
	else
	end
end

