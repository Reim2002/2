while true do
	task.wait()
    local sine = os.clock()
    	d.CFrame = lol.CFrame * CFrame.new(nx,ny,nz) * CFrame.new(math.cos(sine*speedx)*x,math.cos(sine*speedy)*y,math.cos(sine*speedz)*z) * CFrame.Angles(rx,ry,rz) * CFrame.Angles(math.cos(sine*speedxx)*xr,math.cos(sine*speedxy)*xy,math.cos(sine*speedxz)*xz)
end
elseif hatdontfall == true then
game.RunService.Heartbeat:Connect(function()
    local sine = os.clock()
    	d.CFrame = lol.CFrame * CFrame.new(nx,ny,nz) * CFrame.new(math.cos(sine*speedx)*x,math.cos(sine*speedy)*y,math.cos(sine*speedz)*z) * CFrame.Angles(rx,ry,rz) * CFrame.Angles(math.cos(sine*speedxx)*xr,math.cos(sine*speedxy)*xy,math.cos(sine*speedxz)*xz)
end)
end
