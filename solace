local c = game.Players.LocalPlayer.Character
local function gp(parent, name, className)
    local ret = nil
    if parent then
        for i, v in pairs(parent:GetChildren()) do
            if (v.Name == name) and v:IsA(className) then
                ret = v
            end
        end
    end
    return ret
end

local att = gp(gp(gp(c, "MeshPartAccessory", "Accessory"), "Handle", "BasePart"), "att1_Handle", "Attachment")
att.Parent = Handle
att.Position = Vector3.new(0, 2.3, 0)
att.Orientation = Vector3.new(0, 90, -135)

for _,v in pairs(char.Zukain_Katana:GetChildren()) do
	if v:IsA("BasePart") then
		v.Transparency = 1
	end
end

local NewInstance = function(instance,parent,properties)
	local inst = Instance.new(instance,parent)
	if(properties)then
		for i,v in next, properties do
			pcall(function() inst[i] = v end)
		end
	end
	return inst;
end

local HW = NewInstance('Weld',char,{Part0 = la, Part1 = Handle, C0 = CF(0, -1, -.2) * angles(Rad(-25), Rad(0), Rad(-8))})
local HW2 = NewInstance('Weld',char,{Part0 = tors, Part1 = Gold_End, C0 = CF(1, 0, -2) * angles(Rad(116), Rad(180), Rad(0))})

